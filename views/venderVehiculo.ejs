<meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/formulario.css">
    <title>GarageSN</title>
</head>
<header>
<%-include('partials/header.ejs')%>
</header>
       
<p><img src="/img/vendetuauto.png" class="formulario" alt=""></p>


<form action="/venderVehiculo" method="post" enctype="multipart/form-data">
    <div class="formulario">
        <label for="">Titulo</label>
        <input type="text" name="titulo">
    </div>
    <div class="formulario">
        <label for="">Descripción</label>
        <input type="text" name="descripción" class="descripcion">
    </div>
    <div class="formulario">
        <label for="">Kilometros</label>
        <input type="numer" name="km">
    </div>
    <div class="formulario">
        <label for="">Precio</label>
        <input type="number" name="precio">
    </div>
    <div class="formulario">
        <label for="">Categoria</label>
        <select name="categoria">
            <% for (let i = 0; i < categorias.lenght; i++) {%> 
                <option value="<%=categorias[i].id%>">
                    <%=categorias[i].name%> 
                </option>
            <%} %>
        </select>
    </div>
    <button  type="submit"> Crear </button>
</form>


